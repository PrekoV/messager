.Header
    margin 0
    padding: 0
    background-color pink 
    height 60px
    .wrapper-header
        width 90%
        margin 0 auto
        display flex
        justify-content: flex-end
        align-items: center
        padding-top: 20px
        ul
            display flex
            justify-content: flex-end
            align-items: center
            li 
                list-style none
                font-family "Roboto"
                font-size: 16px
                padding: 0 20px
                cursor pointer
                a
                    text-decoration none
                    color #000
                    transition: .4s
                a:hover
                    color white
                    transition: .4s
                    